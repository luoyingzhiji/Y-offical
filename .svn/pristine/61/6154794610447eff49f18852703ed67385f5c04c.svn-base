/*新写的资讯样式*/

.index-news{
    background: #fff;
display: flex;
    padding: 0;
    justify-content: space-between;
    max-width: 1400px;
    margin: 0 auto;
}
.news-left{
   flex-grow: 58;
    overflow: hidden;
    position: relative;
    width: 0;
}
.news-left .news-leftImg{
width: 100%;

}
.news-describe{
    position: absolute;
    top: 49%;
    height: 50%;
    width: 100%;
    background: linear-gradient(0deg,rgba(0,0,0,0.62) 0%, rgba(0,0,0,0.28) 70%,rgba(0,0,0,0.05) 0%);
    padding: 9% 50% 20px 39px;
    color: white;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
}
.news-right{
    display: flex;
    flex-direction: column;
    flex-grow: 40;
    padding-left: 2.3%;
    width: 0;

}
.news-List{

    height: 105px;
    display: flex;
    margin-bottom: 7.2%;
}


.news-List:nth-last-child(1){

    margin-bottom: 0;
}
.index-newsImg{
    width: 0;
    flex-grow: 3;
    margin-right: 4%;
}
.index-news-list-describe{
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    width: 0;
    flex-grow: 7;

}
.index-news-list-describe .title{
    height:65px;
    font-size:2.2rem;
    font-family:PingFang SC;
    font-weight:400;
    line-height:30px;
    color:rgba(51,51,51,1);
    opacity:1;
    overflow: hidden;

    display: -webkit-box;
    word-break: break-all;
    text-overflow: ellipsis;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;

}
.index-news-list-describe .title:hover{

    color: #0a628f;
}
.dym-desc-bottom {
    display: flex;
    justify-content: space-between;
    margin-top: 16px;
    align-items: center;
    font-size: 14px;
    font-family: PingFang SC;
    font-weight: 400;
    color: #333333;
    opacity: 1;
}
.monthAndDate {
    font-size: 21px;
}
.news-describe-text{
    font-size:1.4rem;
    font-family:PingFang SC;
    font-weight:400;
    line-height:2.2rem;
    color:rgba(255,255,255,1);
    display: -webkit-box;word-break: break-all;text-overflow: ellipsis;overflow: hidden; -webkit-box-orient: vertical;-webkit-line-clamp: 3;
}
.news-describe-title{
    font-size:2.4rem;
    font-family:PingFang SC;
    font-weight:400;

    color:rgba(255,255,255,1);
    display: -webkit-box;word-break: break-all;text-overflow: ellipsis;overflow: hidden; -webkit-box-orient: vertical;-webkit-line-clamp: 1;

}
.news-describe-date{
font-size: 1.5rem;
}
